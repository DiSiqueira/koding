.warningPrompt
  background-color      #FFF9FA
  color                 #F2778A
  border-top            1px solid #FCD8DE
  border-bottom         1px solid #FCD8DE
  font-size             13px
  padding               18px 50px 20px 81px
  line-height           20px
  letter-spacing        .04em
  position              relative

  &:before
    content           ""
    background-image  url('data:image/png;base64,')
    background-repeat no-repeat
    position          absolute
    top               26px
    left              40px
    width             24px
    height            22px

  span
    font-weight       600
    text-decoration   underline

  .image
    background          url(/a/images/leaveteam.png) no-repeat
    height              174px
    width               160px
    float               left
    margin-top          83px
    margin-left         10px

  .caption
    width               318px
    float               right
    padding-top         56px
    padding-bottom      73px
    margin-right        21px

  .download-icon
    padding-left        27px
    position            relative

    &:before
      content           ""
      background          url('data:image/png;base64,') no-repeat
      height              20px
      width               20px
      position            absolute
      top                 -2px
      left                0

  strong
    color               inherit
    font-weight         600

  h2
    color               #F2778A
    font-size           20px
    text-align          left
    font-weight         500
    margin-bottom       21px

  p
    font-size           16px
    line-height         20px
    margin-bottom       24px

  a
    font-size           14px
    font-weight         600

&.content-modal.with-partial.kdmodal

main
  margin                31px 40px 30px

strong
  font-weight           600
  color                 inherit

p
  margin-bottom         14px
  line-height           20px

input
  padding-bottom        8px
  margin-bottom         9px

  &.autocomplete
    margin-top          20px

a
  font-weight           300
  font-size             14px

hr
  border                0
  border-top            1px solid #ECEAEA
  margin-top            21px
  margin-bottom         26px

.kdmodal-content header
  border-bottom         0