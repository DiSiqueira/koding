{
  "provider": {
    "softlayer": {
      "username": "{{.Username}}",
      "api_key": "{{.ApiKey}}"
    }
  },
  "output": {
    "key_id": {
      "value": "${softlayer_ssh_key.koding_ssh_key.id}"
    },
    "key_fingerprint": {
      "value": "${softlayer_ssh_key.koding_ssh_key.fingerprint}"
    }
  },
  "resource": {
    "softlayer_ssh_key": {
      "koding_ssh_key": {
        "name": "${var.key_name}",
        "public_key": "${var.public_key}"
      }
    }
  },
  "variable": {
    "key_name": {
      "default": "{{.KeyName}}"
    },
    "public_key": {
      "default": "{{.PublicKey}}"
    }
  }
}
